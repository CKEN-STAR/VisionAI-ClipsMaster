{
  "lazy_loading": {
    "enabled": true,
    "priority_components": [
      "main_window",
      "menu_bar",
      "file_selector"
    ],
    "deferred_components": [
      "advanced_settings",
      "help_dialog",
      "about_dialog"
    ]
  },
  "progressive_loading": {
    "enabled": true,
    "load_stages": [
      {
        "stage": 1,
        "components": [
          "main_window",
          "basic_controls"
        ],
        "target_time": 2.0
      },
      {
        "stage": 2,
        "components": [
          "advanced_features",
          "settings"
        ],
        "target_time": 3.0
      }
    ]
  },
  "caching": {
    "enabled": true,
    "cache_compiled_ui": true,
    "cache_stylesheets": true
  }
}