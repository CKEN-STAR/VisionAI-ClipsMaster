{
    "version": "mobile_2.3",
    "release_date": "2023-04-10",
    "supported_features": [
        "advanced_transitions",
        "timeline_markers",
        "multi_track_export",
        "intelligent_cropping",
        "mobile_optimization",
        "lightweight_effects",
        "power_saving_mode"
    ],
    "feature_config": {
        "advanced_transitions": {
            "enabled": true,
            "max_duration": 6.0,
            "available_presets": [
                "fade",
                "dissolve",
                "wipe",
                "slide",
                "push",
                "mobile_zoom"
            ]
        },
        "timeline_markers": {
            "enabled": true,
            "max_markers": 40,
            "categories": [
                "comment",
                "chapter",
                "error",
                "highlight"
            ]
        },
        "intelligent_cropping": {
            "enabled": true,
            "ai_models": ["face_detection", "mobile_scene_detection"],
            "max_tracked_objects": 4
        },
        "multi_track_export": {
            "enabled": true,
            "max_tracks": 8,
            "formats": ["wav", "mp3", "aac"]
        },
        "mobile_optimization": {
            "enabled": true,
            "battery_optimization": true,
            "reduced_memory_mode": true,
            "target_device_classes": ["high_end", "mid_range", "entry_level"]
        },
        "power_saving_mode": {
            "enabled": true,
            "reduce_resolution": true,
            "disable_realtime_preview": false,
            "cpu_throttling": true
        }
    },
    "compatibility": {
        "min_compatible_version": "mobile_2.0",
        "max_compatible_version": "mobile_3.0",
        "desktop_compatible_version": "2.9.5"
    },
    "deprecated_features": [],
    "metadata": {
        "description": "移动版本，针对手机和平板设备优化",
        "created_by": "VisionAI移动团队"
    }
} 